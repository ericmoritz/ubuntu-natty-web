[uwsgi]
socket=/var/www/servers/{{domain}}/www-data/{{service}}.sock
user=www-data
harakiri=60
master=true
module={{module}}
protocol=uwsgi
virtualenv=/var/www/servers/{{domain}}/code/env
chdir=/var/www/servers/{{domain}}/code/app
env=WRITABLE_ROOT=/var/www/servers/{{domain}}/www-data/data/{{service}}
vacuum=